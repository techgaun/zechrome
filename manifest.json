{
  "name": "Zechrome",
  "version": "0.1",
  "description": "A chrome extension to control your zego powered home",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": "images/logo.png"
  }
}
